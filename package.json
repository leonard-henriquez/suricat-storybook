{
  "name": "suricat-storybook",
  "version": "0.1.0",
  "homepage": "https://github.com/leonard-henriquez/suricat-storybook",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonard-henriquez/suricat-storybook.git"
  },
  "author": {
    "name": "leonard-henriquez"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack --watch --mode development",
    "build": "webpack --mode production --devtool false",
    "prepare": "rm -rf ./dist && yarn build",
    "story:start": "start-storybook -s ./public -p 6006",
    "story:build": "build-storybook -o ./docs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "prettier": "^1.18.2",
    "react": "^16.9.3",
    "react-dom": "^16.9.3",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "react": "^16.9.3",
    "react-dom": "^16.9.3"
  },
  "dependencies": {}
}
